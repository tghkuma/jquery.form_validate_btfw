{
  "name": "jquery.form_validate_btfw",
  "version": "1.8.２",
  "description": "Bootstrap形式のフォーム値のValidateを行う。",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "jsdoc": "^3.6.7",
    "standard": "^16.0.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "test": "eslint js/*.js",
    "jsdoc": "jsdoc js/ -d jsdoc/",
    "compress": "webpack --entry ./js/jquery.btfw.form_validate.js --output-path ./js --output-filename jquery.btfw.form_validate.min.js --mode=\"production\" --devtool source-map",
    "standard": "standard ./js/jquery.btfw.form_validate.js",
    "standard:fix": "standard --fix ./js/jquery.btfw.form_validate.js",
    "super-lint": "docker run -e RUN_LOCAL=true -v `pwd`:/tmp/lint github/super-linter"
  },
  "repository": {
